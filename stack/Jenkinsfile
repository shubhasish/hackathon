pipeline{

agent any
parameters {
        string(name: 'stack', defaultValue: 'dev', description: 'Name of the stack')
    }

stages {

   stage('create'){
   steps {
             script{

                     withCredentials([[$class: 'AmazonWebServicesCredentialsBinding', credentialsId: 'aws']]){
                     sh "export AWS_DEFAULT_REGION=us-east-1"
                     sh "echo ${params.stack}"
                     sh "ls"
                     sh "aws s3 ls"
}
          }


 }}}}